
entity Solucion {
	titulo String
    descripcion String
}

@embedded
entity Componente {
    orden Integer
	titulo String
    descripcion String
    formId String
}


relationship OneToMany {
  Solucion{componentes} to Componente
}

// Set pagination options
paginate Solucion with pagination

// Use Data Transfer Objects (DTO)
dto * with mapstruct

// Set service options to all except few
service all with serviceImpl
